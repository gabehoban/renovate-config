{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
      "config:recommended",
      ":dependencyDashboard",
      ":disableRateLimiting",
      "docker:enableMajor",
      ":enablePreCommit",
      "github>gabehoban/renovate-config:automerge-github-actions",
      "github>gabehoban/renovate-config:commit-message",
      "github>gabehoban/renovate-config:pr-labels",
      "github>gabehoban/renovate-config:semantic-commits",
      "github>gabehoban/renovate-config:custom-managers.json5"
    ],
    "onboarding": false,
    "timezone": "America/Detroit",
    "dependencyDashboardTitle": "Renovate Dashboard",
    "suppressNotifications": ["prIgnoreNotification"],
    "commitBodyTable": true,
    "rebaseWhen": "conflicted",
    "platformCommit": true
}
